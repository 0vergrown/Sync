{
  "type":"apoli:multiple",
  "resource":{
    "type":"apoli:resource",
    "min":0,
    "max":1,
    "hud_render":{
      "should_render":false
    },
    "start_value":0,
    "max_action":{
      "type":"apoli:and",
      "actions":[
        {
          "type":"sync:save_location",
          "id":"entity_save_location",
          "overwrite":true
        },
        {
          "type":"sync:print",
          "message":"Saved The Entity's Location!",
          "show_in_chat":false,
          "logger_id":"Sync/Saved_Entity_Location"
        }
      ]
    }
  },
  "teleport":{
    "type":"apoli:resource",
    "min":0,
    "max":1,
    "hud_render":{
      "should_render":false
    },
    "start_value":0,
    "max_action":{
      "type":"apoli:and",
      "actions":[
        {
          "type":"sync:teleport_to_location",
          "id":"entity_save_location"
        },
        {
          "type":"sync:print",
          "message":"Entity has been teleported!",
          "show_in_chat":false,
          "logger_id":"Sync/Teleport_Entity_To_Location"
        },
        {
          "type":"apoli:modify_resource",
          "modifier":{
            "operation":"set_total",
            "value":0
          },
          "resource":"sync:teleport_entity_to_and_from_location_teleport"
        }
      ]
    }
  }
}